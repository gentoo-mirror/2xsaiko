EGO_SUM=(
	"${EGO_SUM_API[@]}"
	"${EGO_SUM_GITSRHT_DISPATCH[@]}"
	"${EGO_SUM_GITSRHT_KEYS[@]}"
	"${EGO_SUM_GITSRHT_SHELL[@]}"
	"${EGO_SUM_GITSRHT_UPDATE_HOOK[@]}"
)
go-module_set_globals

SRC_URI="
	https://git.sr.ht/~sircmpwn/${MY_PN}/archive/${PV}.tar.gz
	${EGO_SUM_SRC_URI}"

LICENSE="AGPL-3"
SLOT="0"
KEYWORDS="~amd64 ~x86"

DEPEND="
	acct-user/srht-git
	net-libs/nodejs
	dev-lang/sassc"
RDEPEND="${DEPEND}
	dev-python/srht
	dev-python/pygit2
	dev-python/scmsrht
	dev-python/minio
	dev-vcs/git
	www-servers/gunicorn
	cron? ( virtual/cron )
	srht-builds? (
		dev-python/celery
		dev-db/redis
		dev-python/redis-py
		dev-db/redis
		www-servers/gunicorn
	)"
BDEPEND=""

IUSE="+cron srht-builds"

S="${WORKDIR}/${MY_PN}-${PV}"

export PKGVER="${PV}"

src_compile() {
	distutils-r1_src_compile

	cd "${S}/api"
	go build -o gitsrht-api
	cd "${S}/gitsrht-dispatch"
	go build
	cd "${S}/gitsrht-keys"
	go build
	cd "${S}/gitsrht-shell"
	go build
	cd "${S}/gitsrht-update-hook"
	go build
}

src_install() {
	distutils-r1_src_install

	dobin api/gitsrht-api
	dobin gitsrht-dispatch/gitsrht-dispatch
	dobin gitsrht-keys/gitsrht-keys
	dobin gitsrht-shell/gitsrht-shell
	dobin gitsrht-update-hook/gitsrht-update-hook

	newinitd "${FILESDIR}/srht-git" srht-git
	newinitd "${FILESDIR}/srht-git-webhooks" srht-git-webhooks
	newconfd "${FILESDIR}/srht-git.conf" srht-git

	keepdir /var/lib/srht-git
	fowners srht-git:srht-git /var/lib/srht-git
	fperms 750 /var/lib/srht-git

	if use cron; then
		insinto /etc/cron.d
		newins "${FILESDIR}/srht-git.cron" "srht-git"
	fi
}

pkg_postinst() {
	elog 'Upgrading database...'
	srht-migrate git.sr.ht -a upgrade head
	gitsrht-migrate -a upgrade head
}
